{
  "name": "claude-agent-kit",
  "version": "0.1.0",
  "description": "Utilities for @anthropic-ai/claude-agent-sdk: message parsing, session management, UI scaffolding, and runnable examples.",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "lint": "echo \"Add your preferred linter and update scripts\" && exit 0",
    "prepare": "pnpm run build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "anthropic",
    "claude",
    "agent",
    "sdk",
    "session",
    "websocket"
  ],
  "engines": {
    "node": ">=18"
  },
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@anthropic-ai/claude-agent-sdk": ">=0.1.0"
  },
  "dependencies": {
    "nanoid": "^5.0.7",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.1.22",
    "@types/node": "^24.8.1",
    "@types/ws": "^8.18.1",
    "typescript": "^5.4.5",
    "vitest": "^4.0.3"
  }
}
